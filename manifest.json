{
    "manifest_version": 2,
    "name": "Corona",
    "version": "0.1",

    "default_locale": "en",
    "description": "This extension displays the number of active coronavirus cases and related stuff",
    "browser_action": {
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png"
        },
        "default_title": "corona",
        "default_popup": "popup.html"
    }

}